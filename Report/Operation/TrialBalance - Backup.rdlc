    <DataSet Name="TrialBalanceSystemDate">
      <Query>
        <DataSourceName>TrialBalanceDSource</DataSourceName>
        <CommandText>SELECT DATE(system_date) as system_date FROM system_date WHERE is_active = 1 limit 1</CommandText>
      </Query>
      <Fields>
        <Field Name="system_date">
          <DataField>system_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
