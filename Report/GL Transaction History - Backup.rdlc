    <DataSet Name="OpenBalance">
      <Query>
        <DataSourceName>JournalEntryByDateDSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter4.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter5.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ACA.acc_class_id,IFNULL(ABH.balance,0) as open_balance FROM acc_gl_balance_hist ABH LEFT JOIN acc_chat_of_account ACA ON ABH.gl = ACA.gl
WHERE ABH.branch_id = ? AND DATE (ABH.sys_date) = DATE_ADD(?, INTERVAL - 1 DAY) AND ABH.gl = ? LIMIT 1;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
